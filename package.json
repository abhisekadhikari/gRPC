    "type": "commonjs",
    "scripts": {
        "gen": "shx mkdir -p proto/generated && proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=proto/generated ./proto/*.proto",
        "gen:one": "shx mkdir -p proto/generated && proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=proto/generated ./proto/awesome.proto",
        "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
        "dev:server": "ts-node-dev --respawn --transpile-only src/server.ts",
        "dev:client": "ts-node-dev --respawn --transpile-only src/client.ts",
        "start": "ts-node src/main.ts"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.14.0",
        "@grpc/proto-loader": "^0.8.0"
    },
    "devDependencies": {
        "@types/node": "^24.9.1",
        "shx": "^0.4.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "ts-proto": "^2.8.1",
        "typescript": "^5.9.3"
    }
}
